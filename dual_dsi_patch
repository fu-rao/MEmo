+struct platform_device *pdev_hdmi;
+
+struct dsi1_bridge {
+	struct msm_dba_device_info dev_info;
+	int connected;
+	int oled_vddi_gpio;//wangjuan add
+	int oled_vci_gpio;
+	int oled_fd_gpio;
+	int oled_rst_gpio;
+	struct pinctrl *ts_pinctrl;
+	struct pinctrl_state *pinctrl_state_active;
+	struct pinctrl_state *pinctrl_state_suspend;
+};
+
