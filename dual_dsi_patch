+static unsigned char edid_data[] = { //1920*1080
+    //HERABER 8BYTE
+	0x00,0xFF,0xFF,0xFF,0xFF,0xFF,0xFF,0x00, //byte 0-7		heaber	
+	//VENDOR&PRODUCT IDENTIFICATION 10BYTE
+	0x22,0xF0,								 //byte 8-9 	ID Manufacture Name DKX
+	0x01,0x00,								 //byte a-b 	IDproduct Code
+	0x00,0x00,0x00,0x00,					 //byte c-f 	ID Serial Number
+	0x21,									 //byte 10  	Week of Manufacture 33
+	0x1C,									 //byte 11  	Year of Manufacture 1990+28
+	//EDID STRUCTURE VERSION&REVISION 2BYTE
+	0x01,									 //byte 12		version
+	0x04,									 //byte 13		revision
+	//BASIC DISPLAY PARAMETERS /FEATURES 5BYTE
+	0xA2,									 //byte 14		Video Input Definition
+	0x00,									 //byte 15		Max Horizontal Image Size
+	0x00,									 //byte 16		Max Vertical Image Size
+	0x78,									 //byte 17	 	Display Transfer Characteristic(Gamma)
+	0xC7,									 //byte 18		Feature Support(DPMS)
+	//COLOR CHARACTERISTICS 10 BYTE
+	0xE5,									 //byte 19		Red/Green Low Bits
+	0xB5,									 //byte 1a		Blue/White Low Bits
+	0xA3,									 //byte 1b		Red-x
+	0x55,									 //byte 1c		Red-y
+	0x49,									 //byte 1d		Green-x
+	0x99,									 //byte 1e		Green-y
+	0x27,									 //byte 1f		Blue-x
+	0x13,									 //byte 20		Blue-y
+	0x50,									 //byte 21		White-x
+	0x54,									 //byte 22		White-y
+	//ESTABLISHED TIMING 3 BYTE
+	0x00,									 //byte 23		Established timungs 1
+	0x00,									 //byte 24		Established timungs 2
+	0x00,									 //byte 25		Manufacture's Reserved Timings
+	//STANDARD TIMING IDENTIFICATION 16BYTE
+	0x01,0x01,								 //byte 26-27	Standard Timing Identification #1
+	0x00,0x00,								 //byte 28-29	Standard Timing Identification #2
+	0x00,0x00,								 //byte 2a-2b	Standard Timing Identification #3
+	0x00,0x00,								 //byte 2c-2d	Standard Timing Identification #4
+	0x00,0x00,								 //byte 2e-2f	Standard Timing Identification #5
+	0x00,0x00,								 //byte 30-31	Standard Timing Identification #6
+	0x00,0x00,								 //byte 32-33	Standard Timing Identification #7
+	0x00,0x00,								 //byte 34-35	Standard Timing Identification #8
+	//DTD #1
+	0x14,0x0d,//0x99,0x30,					 //p-clock 0x3099=12441 124.41MHz 0x0d14=3348
+											 //panel-width=(panel width h4)&(panel width L8)=0x780=1920
+											 //panel-width=(panel width h4)&(panel width L8)=0x1E0=480
+	0xE0,//0x80,							 //byte 38 		panel width L8 0xe0->(0x80)	
+											 //byte 39      h-back L8(0xbe) h-back=0x0be=190
+	0xbe,//0x2C,							 //byte 39      h-back L8(0x2c) h-back=0x02c=44
+	0x10,//0x70,							 //byte 3a      panel width H4 0x1->(0x7) +h-back H4(0x0)
+											 //panel-height=(panel height H4)&(panel height L8)=0x438=1080
+											 //panel-height=(panel height H4)&(panel height L8)=0x320=800
+	0x20,//0x38,							 //byte 3b		panel height L8 0x20->(0x38)
+											 //byte 3c		v-back L8(0x21) v-back=0x021=33
+	0x21,//0x18,							 //byte 3c		v-back L8(0x18) v-back=0x018=24
+	0x30,//0x40,							 //byte 3d		panel height H4 0x3->(0x4) +v-back h4(0x0)
+											 //byte 3e		h-front L8(0x36) h-front=0x036=54
+	0x36,//0x20,							 //byte 3e		h-front L8(0x20) h-front=0x020=32
+											 //byte 3f		h-pulse L8(0x24) h-pulse=0x024=36
+	0x24,//0x04,							 //byte 3f		h-pulse L8(0x04) h-pulse=0x004=4
+											 //byte 40		v-front L4(0x8)+v-pulse L4(0x5)
+	0x85,//0xC2,							 //byte 40		v-front L4(0xc)+v-pulse L4(0x2)
+	0x00,									 //byte 41 		h-front-h2(00)h-pulse-h2(00)v-front-l2(00)v-pulse-l2(00)
+	0x81,									 //byte 42		h-mm L8 (0x81) H=0x081=129MM
+	0x41,									 //byte 43		v-mm L8 (0x41) V=0x041=65mm
+	0x00,									 //byte 44      h-mm H4(0x00) +v-mm H4(0x00)
+	0x00,
+	0x00,
+	0x14,									 //byte 47		flag 0001 0100
+	//DTD #2
+	0x00,0x00,0x00,0x10,0x00,0x00,0x00,0x00,
+	0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,
+	0x00,0x00,
+	//DTD #3
+	0x00,0x00,0x00,0x10,0x00,0x00,0x00,0x00,
+	0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,
+	0x00,0x00,
+	//DTD #4
+	0x00,0x00,0x00,0x10,0x00,0x00,0x00,0x00,
+	0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,
+	0x00,0x00,
+	
+	0x00,									 //byte 7e				Extenslon Flag
+	0x1B									 //byte 7f				Checksum
+
+};
+static int dsi1_bridge_resume(struct dsi1_bridge *pdata);
+static int dsi1_bridge_suspend(struct dsi1_bridge *pdata);
+static struct dsi1_bridge *dsi1_bridge_get_platform_data(void *client)
+{
+	struct dsi1_bridge *pdata = NULL;
+	struct msm_dba_device_info *dev;
+	struct msm_dba_client_info *cinfo =
+		(struct msm_dba_client_info *)client;
+
+	if (!cinfo) {
+		pr_err("%s: invalid client data\n", __func__);
+		goto end;
+	}
+
+	dev = cinfo->dev;
+	if (!dev) {
+		pr_err("%s: invalid device data\n", __func__);
+		goto end;
+	}
+
+	pdata = container_of(dev, struct dsi1_bridge, dev_info);
+	if (!pdata)
+		pr_err("%s: invalid platform data\n", __func__);
+
+end:
+	return pdata;
+}	
+static int dsi1_bridge_power_on(void *client, bool on, u32 flags)
+{
+	struct dsi1_bridge *pdata = dsi1_bridge_get_platform_data(client);
+
+	pr_info("%s: on:%d\n", __func__, on);
+	if (on) {
+		dsi1_bridge_resume(pdata);
+	} else {
+		dsi1_bridge_suspend(pdata);
+	}
+	return 0;
+}
+static int dsi1_bridge_video_on(void *client, bool on,
+						struct msm_dba_video_cfg *cfg,u32 flags)
+{
+	int ret = 0;
+	struct dsi1_bridge *pdata = dsi1_bridge_get_platform_data(client);
+	pr_info("%s: ------------------------on:%d\n", __func__, on);
+	if (!pdata) {
+		pr_err("%s: invalid platform data\n", __func__);
+		return -EINVAL ;
+	}
+
+	return ret;
+
+}
+
+static int dsi1_bridge_hdcp_enable(void *client, bool hdcp_on, bool enc_on, u32 flags)
+{
+	pr_info("%s: hdcp_on:%d, enc_on:%d\n", __func__, hdcp_on, enc_on);
+	return 0;
+}
+
+static int dsi1_bridge_cec_enable(void *client, bool cec_on, u32 flags)
+{
+	pr_info("%s: cec_on:%d\n", __func__, cec_on);
+	return 0;
+}
+
+static int dsi1_bridge_get_edid_size(void *client, u32 *size, u32 flags)
+{
+	pr_info("%s: entry\n", __func__);
+	if (!size) {
+		pr_err("Wade: %s: return -EINVAL\n", __func__);
+		return -EINVAL;
+	}
+	*size = sizeof(edid_data);
+	return 0;
+}
+
+static int dsi1_bridge_get_raw_edid(void *client, u32 size, char *buf, u32 flags)
+{
+	pr_info("%s: entry\n", __func__);
+	size = size < sizeof(edid_data)?size:sizeof(edid_data);
+	memcpy(buf, edid_data, size);
+	return 0;
+}
+
+static int dsi1_bridge_check_hpd(void *client, u32 flags)
+{
+	pr_info("%s:-------------------------- return 1\n", __func__);
+	return 1;
+}
+
+static int dsi1_bridge_register_dba(struct dsi1_bridge *pdata)
+{
+	struct msm_dba_ops *client_ops;
+	struct msm_dba_device_ops *dev_ops;
+
+	if (!pdata)
+		return -EINVAL;
+
+	pr_info("%s:wj-->\n", __func__);
+
+	client_ops = &pdata->dev_info.client_ops;
+	dev_ops = &pdata->dev_info.dev_ops;
+
+	client_ops->power_on        = dsi1_bridge_power_on;
+	client_ops->video_on        = dsi1_bridge_video_on;
+	client_ops->configure_audio = NULL;
+	client_ops->hdcp_enable     = dsi1_bridge_hdcp_enable;
+	client_ops->hdmi_cec_on     = dsi1_bridge_cec_enable;
+	client_ops->hdmi_cec_write  = NULL;
+	client_ops->hdmi_cec_read   = NULL;
+	client_ops->get_edid_size   = dsi1_bridge_get_edid_size;
+	client_ops->get_raw_edid    = dsi1_bridge_get_raw_edid;
+	client_ops->check_hpd       = dsi1_bridge_check_hpd;
+	client_ops->get_audio_block = NULL;
+	client_ops->set_audio_block = NULL;
+
+	dev_ops->write_reg = NULL;
+	dev_ops->read_reg = NULL;
+	dev_ops->dump_debug_info = NULL;
+
+	strlcpy(pdata->dev_info.chip_name, "dsi1_bridge",
+		sizeof(pdata->dev_info.chip_name));
+
+	mutex_init(&pdata->dev_info.dev_mutex);
+
+	INIT_LIST_HEAD(&pdata->dev_info.client_list);
+
+	return msm_dba_add_probed_device(&pdata->dev_info);
+}
+
+static void dsi1_bridge_unregister_dba(struct dsi1_bridge *pdata)
+{
+	if (!pdata)
+		return;
+
+	msm_dba_remove_probed_device(&pdata->dev_info);
+}
+
+static void dsi1_bridge_notify_clients(struct msm_dba_device_info *dev,
+		enum msm_dba_callback_event event)
+{
+	struct msm_dba_client_info *c;
+	struct list_head *pos = NULL;
+	if (!dev) {
+		pr_err("%s: invalid input\n", __func__);
+		return;
+	}
+
+	list_for_each(pos, &dev->client_list) {
+		pr_info("%s list_for_each\n", __func__);
+		c = list_entry(pos, struct msm_dba_client_info, list);
+
+		pr_info("%s: notifying event %d to client %s\n", __func__,
+			event, c->client_name);
+
+		if (c && c->cb)
+		{
+			pr_info("%s before c->cb()\n", __func__);
+			c->cb(c->cb_data, event);
+		}
+	}
+}
+
+static int dsi1_bridge_resume(struct dsi1_bridge *pdata)
+{
+	pr_info("%s, connected:%d\n", __func__, pdata->connected);
+	if(pdata->connected == 0){
+		dsi1_bridge_notify_clients(&pdata->dev_info, MSM_DBA_CB_HPD_CONNECT);
+		pdata->connected = 1;
+	}
+	return 0;
+}
+
+static int dsi1_bridge_suspend(struct dsi1_bridge *pdata)
+{
+	pr_info("%s, connected:%d\n", __func__, pdata->connected);
+	if(pdata->connected == 1){
+		dsi1_bridge_notify_clients(&pdata->dev_info, MSM_DBA_CB_HPD_DISCONNECT);
+		pdata->connected = 0;
+	}
+	return 0;
+}
+
+static int dsi1_bridge_parse_dt(struct device *dev,
+	struct dsi1_bridge *pdata)
+{
+	struct device_node *np = dev->of_node;
+	u32 tmp = 0;
+	int ret = 0;
+
+	ret = of_property_read_u32(np, "instance_id", &tmp);
+	pdata->dev_info.instance_id = (!ret ? tmp : 0);
+
+	return ret;
+}
+
+static int dsi1_bridge_probe(struct platform_device *pdev)
+{
+	static struct dsi1_bridge *pdata;
+	int ret = 0;
+	pdev_hdmi=pdev;
+	pr_info("%s:dsi1_bridge_probe begin\n", __func__);
+	pdata = devm_kzalloc(&pdev->dev, sizeof(struct dsi1_bridge), GFP_KERNEL);
+	if(!pdata)
+		return -ENOMEM;
+
+	dsi1_bridge_parse_dt(&pdev->dev, pdata);
+
+	platform_set_drvdata(pdev, pdata);
+	ret = dsi1_bridge_register_dba(pdata);
+	if (ret) {
+		pr_err("%s: Error registering with DBA %d\n",
+			__func__, ret);
+		goto err_dba_reg;
+	}
+
+	ret = msm_dba_helper_sysfs_init(&pdev->dev);
+	if (ret) {
+		pr_err("%s: sysfs init failed\n", __func__);
+		goto err_dba_helper;
+	}
+
+	pdata->connected = 0;
+	dev_err(&pdev->dev,"%s success.\n", __func__);
+	return 0;
+
+err_dba_helper:
+	dsi1_bridge_unregister_dba(pdata);
+err_dba_reg:
+	devm_kfree(&pdev->dev, pdata);
+	return ret;
+}
+
+static int dsi1_bridge_remove(struct platform_device *pdev)
+{
+	int ret = -EINVAL;
+	struct dsi1_bridge *pdata;
+
+	if (!pdev)
+		goto end;
+
+	 pdata = platform_get_drvdata(pdev);
+	if (!pdata)
+		goto end;
+
+	msm_dba_helper_sysfs_remove(&pdev->dev);
+	devm_kfree(&pdev->dev, pdata);
+	ret = 0;
+end:
+	return ret;
+}
+
+static struct of_device_id dsi1_bridge_match_table[] = {
+		{ .compatible = "qcom,dsi1_bridge",},
+		{ },
+};
+
+static struct platform_driver dsi1_bridge_driver = {
+	.probe = dsi1_bridge_probe,
+	.remove = dsi1_bridge_remove,
+	.driver = {
+		.name = "dsi1_bridge",
+		.owner = THIS_MODULE,
+		.of_match_table = dsi1_bridge_match_table,
+	},
+};
+
+module_platform_driver(dsi1_bridge_driver);
+
+MODULE_DESCRIPTION("Dsi1 bridge driver for dual mipi dsi");
+MODULE_LICENSE("GPL");
+MODULE_VERSION("0.1");
+
